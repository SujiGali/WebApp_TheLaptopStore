<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.RegistrationServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>DealerRegistrationServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.DealerRegistrationServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.LoginServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>DealerLoginServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.DealerLoginServlet</servlet-class>
    </servlet>
    
    <servlet>
    <servlet-name>CartServlet</servlet-name>
    <servlet-class>uncc.tls.servlets.CartServlet</servlet-class>
  </servlet>
    
  <servlet>
        <servlet-name>BillingAddressServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.BillingAddressServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ShippingAddressServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.ShippingAddressServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>PaymentServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.PaymentServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ProductServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.ProductServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>FilterSearchServlet</servlet-name>
        <servlet-class>uncc.tls.servlets.FilterSearchServlet</servlet-class>
    </servlet>
    
     
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DealerLoginServlet</servlet-name>
        <url-pattern>/DealerLoginServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DealerRegistrationServlet</servlet-name>
        <url-pattern>/DealerRegistrationServlet</url-pattern>
    </servlet-mapping>
     
    <servlet-mapping>
    <servlet-name>CartServlet</servlet-name>
    <url-pattern>/CartServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>BillingAddressServlet</servlet-name>
        <url-pattern>/BillingAddressServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ShippingAddressServlet</servlet-name>
        <url-pattern>/ShippingAddressServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>PaymentServlet</servlet-name>
        <url-pattern>/PaymentServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name>
        <url-pattern>/ProductServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>FilterSearchServlet</servlet-name>
        <url-pattern>/FilterSearchServlet</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
